[user]
    name = edurso
    email = eric@casadurso.com
[filesystem "Ubuntu|11.0.7|rootfs"]
    timestampResolution = 26000 nanoseconds
    minRacyThreshold = 4333 microseconds
[diff]
    submodule = log
[status]
    submoduleSummary = true
[push]
    recurseSubmodules = on-demand
[submodule]
    recurse = true
[core]
    editor = nvim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
